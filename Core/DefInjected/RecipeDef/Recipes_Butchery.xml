  <!-- EN: Butchering. -->
  <ButcherCorpseFlesh.jobString>Oprawianie.</ButcherCorpseFlesh.jobString>
  
  <!-- EN: disassemble mechanoid -->
  <ButcherCorpseMechanoid.label>rozmontuj mechanoida</ButcherCorpseMechanoid.label>
