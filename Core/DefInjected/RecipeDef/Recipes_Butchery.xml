<LanguageData>
  
  <!-- EN: butcher creature -->
  <ButcherCorpseFlesh.label>opraw stworzenie</ButcherCorpseFlesh.label>
  <!-- EN: Butcher a creature to obtain its meat, leather, and other body products. -->
