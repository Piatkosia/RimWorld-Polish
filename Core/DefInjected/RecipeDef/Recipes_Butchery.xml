  <ButcherCorpseMechanoid.description>Rozmontuj zniszczonego mechanoida na komponenty.</ButcherCorpseMechanoid.description>
  <!-- EN: Disassembling mechanoid. -->
  <ButcherCorpseMechanoid.jobString>Rozmontowywanie mechanoida.</ButcherCorpseMechanoid.jobString>
  
